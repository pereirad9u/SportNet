{{dump(test)}}

<div id="index-banner" class="parallax-container" style="width: 100%;height: auto">
    <div class="section no-pad-bot">
        <div class="container ">
            <br><br>
            <h1 class="header center white-text text-lighten-2">SportNet</h1>
            <div class="row center">
                <h5 class="header span-12 white-text light"></h5>
            </div>
            <br><br>

        </div>
    </div>
    <div class="parallax"><img src="images/home.jpg"></div>
</div>

<div class="container">
    <div class="section">
        <h5>Votre prochain événement :</h5>
        <div class="row">
            <div class="card blue-grey darken-1 col s12 m5 l5 offset-m4 offset-l4">
                <div class="card-content white-text">

                    <span class="card-title">{{ e.nom }}</span>
                    <p>Date : {{ e.date_debut }}</p>
                    <p>Nombre participant : {{ e.nb_participants}}</p>
                    <p>Nombre d'épreuves : {{e.nb_epreuve}}</p>
                    <p>Etat : {{e.etat}}</p>

                </div>
                <div class="card-action">

                    <a href="/anEventOrg/{{e.id}}">Gérer</a>

                </div>
            </div>
        </div>
    </div>
    <div class="section">
        <h5>Vos événements passés :</h5>
        <div class="row">
          {% for last_ev in last_events %}
          <div class="card blue-grey darken-1 col s12 m5 offset-m1 l5 offset-l1">
              <div class="card-content white-text">
                  <span class="card-title">{{last_ev.nom}}</span>
                  <p>Date début : {{last_ev.date_debut}}</p>
                  <p>Date fin : {{last_ev.date_fin}}</p>
                  <p>Nombre participant : {{last_ev.nb_participant}}</p>
                  <p>Etat : {{last_ev.etat}}</p>
              </div>
              <div class="card-action">
                  <a href="/anEventOrg/{{last_ev.id}}">Gérer</a>
              </div>
          </div>
          {% endfor %}
        </div>
    </div>

</div>
