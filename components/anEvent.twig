


<div class="container ">
    <div class="row ">
        <div class="center">
            <h3> {{ event.name }}</h3>
        </div>
        <div class="center">

            <div class="carousel center " data-indicators="true">
                {% for epreuve in tabEpreuve %}

                    <a class="carousel-item" href="#one!"><h6>{{ epreuve.nom }}</h6><img src="/{{ epreuve.image }}"></a>

                {% endfor %}

            </div>

        </div>

    </div>

    <div class="row ">
        <div class="col s6 card">
            <p>{{ event.description }}</p>
        </div>
        <div class=" col s6 card">
            <p>{{ organiser.nom }}  {{ organiser.prenom }}</p>
            <p>Telephone : {{ organiser.telephone }} </p>
            <p>Email : {{  organiser.email }}</p>
            {%if organiser.association is defined %}
            <p>Site :<a href="{{ organiser.siteweb }}" >{{  organiser.siteweb }}</a></p>
            {% endif %}
        </div>
    </div>

    <div class="row">
        {% for epreuve in tabEpreuve%}
        <div class="col s4 card">
            <h6> {{ epreuve.nom }}</h6>
            <p>{{ epreuve.description }} </p>
            <p>Discipline : {{ epreuve.discipline }} </p>
            <p>Date : {{ epreuve.date }} </p>
            <p>Nombre de participant maximum : {{ epreuve.nb_participant_max }} </p>
            <p>Nombre de participant  : {{ epreuve.nb_participant }} </p>
            <p>Prix : {{ epreuve.prix }} </p>

        </div>

        {% endfor %}

    </div>

</div>